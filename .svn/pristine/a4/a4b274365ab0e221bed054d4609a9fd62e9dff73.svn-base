@charset "UTF-8";
 $BtnColor:#FF4650; /*red*/
 $BtnDisable:#f8f8f8; /*gray*/
 .normal{
 	box-sizing: border-box;
    text-align: center;
    border-radius: 5px;
    -moz-border-radius: 5px;
    -o-border-radius: 5px;
    -webkit-border-radius: 5px;
    background:$BtnColor;
    color: #FFF;
    height: 44px;
    width: 100%;
    padding: 0;
    line-height: 44px;
    font-size: 15px;
    display: inline-block !important;
    border: none !important;
 }
 .BtnDisable {
    box-sizing: border-box;
    text-align: center;
    border-radius: 5px;
    -moz-border-radius: 5px;
    -o-border-radius: 5px;
    -webkit-border-radius: 5px;
    background: $BtnDisable;
    color: #FFF;
    height: 44px;
    width: 100%;
    padding: 0;
    line-height: 44px;
    font-size: 15px;
    display: inline-block !important;
    border: none !important;
}
